includes:
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon
    - vendor/phpstan/phpstan-strict-rules/rules.neon
parameters:
    level: 9
    # WpThemeMagicPropertiesClassReflectionExtension needs WP_Theme
    scanDirectories:
        - vendor/johnpbloch/wordpress-core
    paths:
        - bootstrap.php
        - src/
        - tests/
    excludePaths:
        - tests/data/
